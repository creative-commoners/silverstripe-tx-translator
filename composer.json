{
    "name": "silverstripe/tx-translator",
    "description": "Silverstripe TX Translator",
    "type": "silverstripe-vendormodule",
    "authors": [
        {
            "name": "SilverStripe",
            "homepage": "http://silverstripe.com"
        },
        {
            "name": "The SilverStripe Community",
            "homepage": "http://silverstripe.org"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.1",
        "silverstripe/framework": "^4.12 || ^5.0",
        "silverstripe/vendor-plugin": "^1 || ^2",
        "symfony/process": "^4 || ^6",
        "symfony/yaml": "^4 || ^6",
        "symfony/dotenv": "^4 || ^6",
        "guzzlehttp/guzzle": "^6 || ^7"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.7"
    },
    "autoload": {
        "psr-4": {
            "SilverStripe\\TxTranslator\\": "src/"
        }
    },
    "scripts": {
        "lint": "phpcs src/ tests/php/",
        "lint-clean": "phpcbf src/ tests/php/"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
